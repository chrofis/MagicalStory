{
  "name": "magical-story",
  "version": "2.0.0",
  "description": "AI-powered personalized story generator with authentication",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "dev:log": "node server.js 2>&1 | tee logs/server.log",
    "dev:client": "cd client && npm run dev",
    "dev:python": "venv311/Scripts/python.exe photo_analyzer.py",
    "build": "cd client && npm install && npm run build",
    "build:client": "cd client && npm run build",
    "test": "npx playwright test tests/local-debug.spec.ts --project=chromium --workers=1",
    "test:local": "cross-env TEST_BASE_URL=http://localhost:5173 npx playwright test tests/local-debug.spec.ts --project=chromium --workers=1",
    "test:prod": "npx playwright test tests/local-debug.spec.ts --project=chromium --workers=1",
    "test:headed": "cross-env TEST_BASE_URL=http://localhost:5173 npx playwright test tests/local-debug.spec.ts --project=chromium --workers=1 --headed",
    "test:all": "npx playwright test --project=chromium --workers=1",
    "test:inpaint": "node test-inpaint.js",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:ui": "vitest --ui",
    "test:api": "vitest run tests/api",
    "test:e2e:avatar": "npx playwright test tests/avatar.spec.ts --project=chromium --workers=1",
    "test:e2e:avatar:headed": "cross-env TEST_BASE_URL=http://localhost:5173 npx playwright test tests/avatar.spec.ts --project=chromium --workers=1 --headed",
    "test:concurrency": "npx playwright test tests/concurrency --workers=1",
    "test:avatar:all": "npm run test:unit && npm run test:api && npm run test:e2e:avatar"
  },
  "keywords": [
    "storytelling",
    "ai",
    "children",
    "personalized"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "firebase-admin": "^13.6.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "p-limit": "^3.1.0",
    "pdfkit": "^0.17.2",
    "pg": "^8.11.3",
    "resend": "^6.6.0",
    "sharp": "^0.33.5",
    "stripe": "^20.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/node": "^25.0.3",
    "@vitest/ui": "^4.0.17",
    "cross-env": "^10.1.0",
    "nodemon": "^3.0.1",
    "vitest": "^4.0.17"
  }
}
